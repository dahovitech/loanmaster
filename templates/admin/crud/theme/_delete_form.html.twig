<form action="{{ path('app_crud_delete', {'entityName': 'Theme', 'id': theme.id}) }}" method="post" style="display: inline-block;">
        <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ theme.id) }}">

    <button type="submit" class="btn btn-sm btn-danger">{{ 'delete'|trans }}</button>
</form>
