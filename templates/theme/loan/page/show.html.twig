{% extends '@theme/'~setting('theme')~'/base.html.twig' %}

{% block title %}
	{{page.title|raw|trim}}
{% endblock %}
{% block description %}
	{{page.resume|raw|trim}}
{% endblock %}
{% block canonical %}
	{{ url('page_show',{slug: page.slug})|trim }}
{% endblock %}

{% block ogTitle %}
	{{page.title|raw|trim}}
{% endblock %}
{% block ogDescription %}
	{{page.resume|raw|trim}}
{% endblock %}
{% block ogImage %}
	{{ absolute_url(page.image|default('#'))|trim }}
{% endblock %}
{% block ogUrl %}
	{{ url('page_show',{slug: page.slug})|trim }}
{% endblock %}

{% block twitterTitle %}
	{{page.title|trim}}
{% endblock %}
{% block twitterDescription %}
	{{page.resume|raw|trim}}
{% endblock %}
{% block twitterImage %}
	{{ absolute_url(page.image|default('#'))|trim }}
{% endblock %}
{% block twitterUrl %}
	{{ url('page_show',{slug: page.slug})|trim }}
{% endblock %}

{% block body %}
	<!--<< Breadcrumb Section Start >>-->
	<div class="breadcrumb-wrapper section-padding bg-cover" style="background-image: url({{ asset(page.image) }});">
		<div class="container">
			<div class="page-heading">
				<h1 class="wow fadeInUp" data-wow-delay=".3s">{{page.title|raw}}</h1>
				<ul class="breadcrumb-items wow fadeInUp" data-wow-delay=".5s">
					<li>
						<a href="{{path('home')}}">
							{{"front.menu.home"|trans}}
						</a>
					</li>
					<li>
						<i class="fal fa-minus"></i>
					</li>
					<li>
						{{page.title|raw}}
					</li>
				</ul>
			</div>
		</div>
	</div>

	<!--<< Visa Details Section Start >>-->
	<section class="visa-details-section fix section-padding">
		<div class="container">
			<div class="visa-details-wrapper">
				<div class="page-details-items">
							
							<div class="details-content">								{{page.content|raw}}
								
							</div>
						</div>
			</div>
		</div>
	</section>
		{{ include('@theme/'~setting('theme')~'/inc/action1.html.twig') }}

	{{ include('@theme/'~setting('theme')~'/inc/testimonial1.html.twig') }}

{% endblock %}
