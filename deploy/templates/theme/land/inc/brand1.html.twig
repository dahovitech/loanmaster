<!--<< Brand Section Start >>-->
	<div class="brand-section fix section-padding">
		<div class="container">
			<div class="brand-wrapper">
				<h6 class="text-center wow fadeInUp" data-wow-delay=".3s">{{"front.brand.title"|trans}}</h6>
				<div class="brand-carousel-active">
					{% for brand in brands() %}
						<div class="brand-image">
							<img width="140" height="122" src="{{brand.image}}" alt="brand-img">
						</div>
					{% endfor %}

				</div>
			</div>
		</div>
	</div>